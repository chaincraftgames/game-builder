    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.14",
    "@langchain/core": "^0.3.40",
    "@langchain/langgraph": "^0.2.49",
    "@langchain/langgraph-checkpoint-sqlite": "^0.1.4",
    "@langchain/openai": "^0.4.4",
    "acorn": "^8.15.0",
    "discord.js": "^14.18.0",
